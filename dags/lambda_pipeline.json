{
  "nodes": [
    {
      "id": "fetch_book_1",
      "type": "lambda",
      "config": {
        "function_name": "cascade-fetch-book",
        "timeout": 300,
        "payload": {
          "book_id": "book_1",
          "book_title": "Pride and Prejudice"
        }
      }
    },
    {
      "id": "fetch_book_2",
      "type": "lambda",
      "config": {
        "function_name": "cascade-fetch-book",
        "timeout": 300,
        "payload": {
          "book_id": "book_2",
          "book_title": "Jane Eyre"
        }
      }
    },
    {
      "id": "extract_chapters_1",
      "type": "lambda",
      "config": {
        "function_name": "cascade-extract-chapters",
        "timeout": 300
      },
      "depends_on": ["fetch_book_1"]
    },
    {
      "id": "extract_chapters_2",
      "type": "lambda",
      "config": {
        "function_name": "cascade-extract-chapters",
        "timeout": 300
      },
      "depends_on": ["fetch_book_2"]
    },
    {
      "id": "word_frequency_1",
      "type": "lambda",
      "config": {
        "function_name": "cascade-word-frequency",
        "timeout": 300,
        "payload": {
          "top_n": 500,
          "analysis_depth": "basic"
        }
      },
      "depends_on": ["extract_chapters_1"]
    },
    {
      "id": "word_frequency_2",
      "type": "lambda",
      "config": {
        "function_name": "cascade-word-frequency",
        "timeout": 300,
        "payload": {
          "top_n": 500,
          "analysis_depth": "basic"
        }
      },
      "depends_on": ["extract_chapters_2"]
    },
    {
      "id": "compare_books",
      "type": "lambda",
      "config": {
        "function_name": "cascade-compare-books",
        "timeout": 300
      },
      "depends_on": ["word_frequency_1", "word_frequency_2"]
    }
  ],
  "edges": [
    {"from": "fetch_book_1", "to": "extract_chapters_1"},
    {"from": "fetch_book_2", "to": "extract_chapters_2"},
    {"from": "extract_chapters_1", "to": "word_frequency_1"},
    {"from": "extract_chapters_2", "to": "word_frequency_2"},
    {"from": "word_frequency_1", "to": "compare_books"},
    {"from": "word_frequency_2", "to": "compare_books"}
  ],
  "description": "Literary analysis pipeline comparing two books using Lambda functions",
  "enabled": true
}
